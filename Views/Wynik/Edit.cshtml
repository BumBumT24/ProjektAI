@model ProjektCRUD20510.Models.Wynik

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Wynik</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit" asp-controller="Wynik">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Id_Testu" class="control-label"></label>
                <select asp-for="Id_Testu" class="form-control" asp-items="@(ViewData["Id_Testu"] as SelectList)" required></select>
                <span asp-validation-for="Id_Testu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Id_Uzytkownika" class="control-label"></label>
                <select asp-for="Id_Uzytkownika" class="form-control" asp-items="@(ViewData["Id_Uzytkownika"] as SelectList)" required></select>
                <span asp-validation-for="Id_Uzytkownika" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Id_Kategorii" class="control-label"></label>
                <select asp-for="Id_Kategorii" class="form-control" asp-items="@(ViewData["Id_Kategorii"] as SelectList)" required></select>
                <span asp-validation-for="Id_Kategorii" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="wynik" class="control-label"></label>
                <input asp-for="wynik" class="form-control" required />
                <span asp-validation-for="wynik" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" asp-controller="Wynik">Powrót</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}