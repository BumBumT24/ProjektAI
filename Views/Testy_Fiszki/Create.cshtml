                <span asp-validation-for="Id_Testu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Id_Fiszki" class="control-label"></label>
                <select asp-for="Id_Fiszki" class="form-control" asp-items="@(ViewData["Id_Fiszki"] as SelectList)" required>
                    <option value="">Please select a fiszka</option>
                </select>
                <span asp-validation-for="Id_Fiszki" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
