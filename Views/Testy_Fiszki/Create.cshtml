@model ProjektCRUD20510.Models.Testy_Fiszki

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Testy_Fiszki</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" asp-controller="Testy_Fiszki">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Id_Testu" class="control-label"></label>
