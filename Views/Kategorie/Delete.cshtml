@model ProjektCRUD20510.Models.Kategorie

@{
    ViewData["Title"] = "Delete";
}

<h1>Usuń</h1>

<h3>Czy napewno chcesz usunać?</h3>
<div>
    <h4>Kategorie</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nazwa)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nazwa)
        </dd>
    </dl>

    <form asp-action="DeleteConfirmed" asp-controller="Kategorie">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Usuń" class="btn btn-danger" /> |
        <a asp-action="Index" asp-controller="Kategorie">Powrót</a>
    </form>
</div>